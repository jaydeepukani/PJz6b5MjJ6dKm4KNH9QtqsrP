{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-mongodb": "*",
        "barryvdh/laravel-snappy": "^1.0.0",
        "dipeshsukhia/laravel-country-state-city-data": "^2.0",
        "facebook/graph-sdk": "^5.1",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.9",
        "guzzlehttp/guzzle": "^7.0.1",
        "jenssegers/mongodb": "^3.8",
        "laravel/breeze": "^1.1",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.11",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "owen-oj/laravel-getid3": "^2.0",
        "propaganistas/laravel-phone": "^4.3",
        "spatie/laravel-medialibrary": "^10.0",
        "stripe/stripe-php": "^7.119"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helper/Common.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php -r \"shell_exec('cp -rf prevendor/* vendor/');\"",
            "@php artisan stub:publish"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
